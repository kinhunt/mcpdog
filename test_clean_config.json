{
  "servers": {
    "filesystem": {
      "name": "filesystem",
      "enabled": true,
      "transport": "stdio",
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-filesystem",
        "/tmp"
      ],
      "timeout": 30000,
      "retries": 3,
      "toolsConfig": {
        "mode": "blacklist"
      }
    }
  },
  "version": "2.0.0",
  "logging": {
    "level": "info"
  }
}